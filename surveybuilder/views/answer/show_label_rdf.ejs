<LabelAnswer>
    <% if (lineitem.answerText) { %>
        <answerText><%= lineitem.answerText %></answerText>
    <% } %>
    <% if (lineitem.answerProperty) { %>
        <answerProperty rdf:resource="<%= lineitem.answerProperty %>" />
    <% } %>
    <% if (lineitem.answerObject) { %>
        <answerObject rdf:resource="<%= lineitem.answerObject %>" />
    <% } %>
    <% if (lineitem.answerValue) { %>
        <answerObject <% if (lineitem.dataType && lineitem.dataType !== '') { %> rdf:datatype="<%= lineitem.dataType %>" <% } %>><%= lineitem.answerValue %></answerObject>
    <% } %>
    <% if (lineitem.answerNote) { %>
        <answerNote><%= lineitem.answerNote %></answerNote>
    <% } %>
</LabelAnswer>
