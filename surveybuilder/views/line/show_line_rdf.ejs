<Line rdf:about="<%= line.about %>">
	<% if (line.internalName) { %>
        <internalName><%= line.internalName %></internalName>
    <% } %>

    <% if (line.title) { %>
        <dc:title><%= line.title %></dc:title>
    <% } %>

    <% if (line.questionsPerPage) { %>
        <questionsPerPage><%= line.questionsPerPage %></questionsPerPage>
    <% } %>

    <% if (line.childId) { %>
        <%== $.View('//surveybuilder/views/lineitem/show_rdf', {lineitem:Lineitem.findOne({id:line.childId})}) %>
    <% } %>

</Line>
